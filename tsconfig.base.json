{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@myorg/api": ["directory=libs/api/src/index.ts"],
      "@myorg/frontend/pages/consultant": [
        "libs/frontend/pages/consultant/src/index.ts"
      ],
      "@myorg/frontend/pages/consultant/pages": [
        "libs/frontend/pages/consultant/pages/src/index.ts"
      ],
      "@myorg/frontend/pages/dashboard": [
        "libs/frontend/pages/dashboard/src/index.ts"
      ],
      "@myorg/frontend/pages/dashboard/pages": [
        "libs/frontend/pages/dashboard/pages/src/index.ts"
      ],
      "@myorg/frontend/pages/department": [
        "libs/frontend/pages/department/src/index.ts"
      ],
      "@myorg/frontend/pages/patients": [
        "libs/frontend/pages/patients/src/index.ts"
      ],
      "@myorg/frontend/pages/patients/pages": [
        "libs/frontend/pages/patients/pages/src/index.ts"
      ],
      "@myorg/frontend/pages/users": ["libs/frontend/pages/users/src/index.ts"],
      "@myorg/frontend/pages/users/pages": [
        "libs/frontend/pages/users/pages/src/index.ts"
      ],
      "@myorg/frontpage/pages/users/pages": [
        "libs/frontpage/pages/users/pages/src/index.ts"
      ],
      "@myorg/login": ["libs/frontend/login-page/src/index.ts"],
      "@myorg/page": ["page/src/index.ts"],
      "@myorg/pages": ["libs/frontend/pages/login/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
